
@{
    ViewData["Title"] = "نتیجه پرداخت";
}



<div class="container">
    @if (ViewBag.issuccess == true)
    {
        <div class="alert alert-success">
            <h2>پرداخت با موفقیت انجام شد</h2>
            <p> کد پیگیری :@ViewBag.code</p>
        </div>
    }
    else
    {
        <div class="alert alert-danger">
             با مشکل مواجه شد
        </div>
    }
</div>